#include<bits/stdc++.h>
#include "graph.h" // using namespace already included
int main(){
	
	//take the input from the graph
	Graph g1("input.txt",false); // undirected graph
	Utilities u;
	cout<<"Original ";
	g1.show();
	Graph dense = g1.getDensestSub(); // using goldberg's method
	cout<<"Densest Sub ";
	dense.show();
	Graph approxdense = g1.getApproxDenseSub();
	cout<<"Approximate dense sub ";
	approxdense.show();

}

